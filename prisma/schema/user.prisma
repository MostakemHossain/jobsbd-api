

model User {
id String @id @unique @default(uuid())
email String
phoneNumber String
password String
role Role
status STATUS
candidate Candidate?
employer Employer? 
createdAt DateTime @default(now())
updatedAt DateTime @updatedAt

}


enum Role {
    ADMIN
    SUPER_ADMIN
    CANDIDATE
    EMPLOYER
}

enum STATUS {
    PENDING
    ACTIVE
    BLOCKED

}